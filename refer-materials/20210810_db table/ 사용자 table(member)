
create type division as enum  ('nurse','doctor','admin')



create table member (
	no serial primary key,
	name varchar(45),
	id varchar(45),
	password varchar(45), 
	registration_no varchar(45),
	email varchar(45),
	cellphone varchar(45),
	role division,
	profile varchar (500),
	account varchar(45), 
	working boolean
	)
	
	
	 
	
	
